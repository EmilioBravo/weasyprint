{
  "name": "rockett/weasyprint",
  "description": "A simple Laravel wrapper for the WeasyPrint PDF Engine.",
  "type": "library",
  "license": "ISC",
  "authors": [
    {
      "name": "Mike Rockett",
      "email": "mike@rockett.pw"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.1.3",
    "symfony/process": "^4.3|^5.0",
    "illuminate/support": "^5.8|^6.0|^7.0"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "WeasyPrint\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "WeasyPrint\\WeasyPrintProvider"
      ]
    }
  },
  "scripts": {
    "test": [
      "@cda",
      "@phpunit"
    ],
    "cda": "composer dumpautoload",
    "phpunit": "./vendor/bin/phpunit --testdox"
  }
}
